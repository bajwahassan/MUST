<%= render "errors" %>
<%= form_for @movie do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title, class:"form-control" %><br>

  <%= f.label :genre %>
  <%= f.text_field :genre, class:"form-control"%><br>

  <%= f.label :description %>
  <%= f.text_field :description, class:"form-control" %><br>

  <%= f.label :year %>
  <%= f.text_field :year, class:"form-control" %><br>

  <%= f.submit "update movie", class: "btn btn-primary" %>

<% end %>
